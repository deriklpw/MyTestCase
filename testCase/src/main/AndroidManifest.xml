        <activity android:name=".c_third.viewpager.TabHostActivity" />
        <activity android:name=".c_third.sensor.SensorTestActivity" />
        <activity android:name=".c_third.jnitest.JniTestActivity" />
        <activity android:name=".c_third.resource.ResourceTestActivity" />
        <activity android:name=".c_third.resource.MenuTestActivity" />
