    <TextView
        android:id="@+id/third_resource_textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="text3"/>

    <Button
        android:id="@+id/third_resource_button1"
        android:layout_width="wrap_content"
