        android:id="@+id/sensor_text_view8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="text"
        android:layout_centerHorizontal="true"/>

    <ImageView
        android:id="@+id/sensor_znz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/znz"/>
</LinearLayout>
